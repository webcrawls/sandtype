@namespace TerryTyper
@using System
@using Sandbox.UI
@inherits GamePageBase
@attribute [StyleSheet]

<root>
	<div class="header">
		<h1>@PageName</h1>
		<button class="exit-button" @onclick=@(HandleClose)>X</button>
	</div>
	<div>
		<div slot="navigator-canvas">
		</div>
	</div>
</root>

@code {

	protected override int BuildHash()
	{
		return HashCode.Combine(PageName);
	}

}
